{
  "$schema": "https://raw.githubusercontent.com/nicoverbruggen/tauri-settings-schema/v2/schema.json",
  "productName": "ICQ26a",
  "version": "0.1.2",
  "identifier": "com.icq26a.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "ICQ26a",
        "width": 300,
        "height": 480,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
